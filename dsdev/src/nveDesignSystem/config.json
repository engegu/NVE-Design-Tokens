{
  "source": ["tokens/configFiles/**/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "files": [{
        "destination": "tokens/_variables.scss",
        "format": "scss/variables"
      }]
    },
    "css": {
      "transformGroup": "css",
      "files": [{
        "destination": "tokens/variables.css",
        "format": "css/variables"
      }]
    }
  }
}